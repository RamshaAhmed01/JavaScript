        var num1 = 20;
        var num2 = 4;
        
        var subtract = num1 - num2;
        document.write("The difference of " + num1 + " and " + num2 + " is " + subtract + "<br>");

        var multiply = num1 * num2;
        document.write("The product of " + num1 + " and " + num2 + " is " + multiply + "<br>");

        var divide = num1 / num2;
        document.write("The quotient of " + num1 + " and " + num2 + " is " + divide + "<br>");

        var modulus = num1 % num2;
        document.write("The modulus of " + num1 + " and " + num2 + " is " + modulus);
    </script>
</body>
</html>

