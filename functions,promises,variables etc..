<!DOCTYPE html>
<html>
<head>
    <title>Task 9</title>
</head>
<body>
    <script>
        var num1 = parseFloat(prompt("Enter the first number:"));
        var num2 = parseFloat(prompt("Enter the second number:"));
        var operation = prompt("Enter the operation (+, -, *, /, %):");
        var result;
        
        if (operation === "+") {
            result = num1 + num2;
        } else if (operation === "-") {
            result = num1 - num2;
        } else if (operation === "*") {
            result = num1 * num2;
        } else if (operation === "/") {
            result = num1 / num2;
        } else if (operation === "%") {
            result = num1 % num2;
        } else {
            alert("Invalid operation");
        }
        
        if (result !== undefined) {
            alert("The result of " + num1 + " " + operation + " " + num2 + " is " + result);
        }
    </script>
</body>
</html>


