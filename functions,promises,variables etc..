<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<title>Display Messages in Sequence</title>
</head>
<body>

<script>

var messages = [
    "Welcome to our website!",
    "We hope you are fine.",
    "Feel free to contact!"
];

function displayMessages() {
   
    for (var i = 0; i < messages.length; i++) {

        setTimeout(function(index) {
            return function() {
                alert(messages[index]);
            };
        }(i), i * 2000); 
    }
}


displayMessages();
</script>

</body>
</html>

